{
  "name": "@moneyforward/cloud-react-ui",
  "version": "0.0.1",
  "description": "React ui components for Money Forward Cloud",
  "author": "Money Forward Cloud Engineers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/moneyforward/cloud-react-ui.git"
  },
  "bugs": {
    "url": "https://github.com/moneyforward/cloud-react-ui/issues"
  },
  "homepage": "https://github.com/moneyforward/cloud-react-ui#readme",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib/",
    "build": "tsc",
    "prepare": "tsc",
    "lint": "prettier --check '**/*.{ts,tsx,json}' && eslint src/ --ext .ts,.tsx",
    "fmt": "prettier --write '**/*.{ts,tsx,json}' && eslint src/ --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "storybook": "start-storybook -c .storybook/ --quiet",
    "storybook:deploy": "storybook-to-ghpages"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@material-ui/core": "^4.12.3",
    "deepmerge": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-select": "^4.3.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.10",
    "@types/react-select": "^4.0.18",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-moneyforward": "^1.0.0",
    "jest": "^27.3.1",
    "jest-styled-components": "^7.0.6",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7"
  }
}
